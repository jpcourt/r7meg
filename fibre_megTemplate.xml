	<modifElemArray>
		<canal.det.modele.gestion.ModifElem>
			<situationFlash>
				<abonnementArray>
					<% _.each(productArray, function(product) { %>
						<canal.det.modele.gestion.Abonnement>
							<dateValiditeDebut>
								<time>1379921874000</time>
								<timezone>GMT</timezone>
							</dateValiditeDebut>
							<canal.det.modele.gestion.OffreDroit>
								<idOffreDroits>
									<font color=red><b><%= product %></b></font>
								</idOffreDroits>
								<operateurCommercial>CSA</operateurCommercial>
								<vecteur>10</vecteur>
								<x>false</x>
